<div class="log">
	<div class="am-g">
		<div class="am-u-lg-3 am-u-md-6 am-u-sm-8 am-u-sm-centered log-content">
			<h1 class="log-title am-animation-slide-top"></h1>
			<br>
			<form class="am-form" id="signup_form" method="post">
				<div class="am-input-group am-radius am-animation-slide-left">
					<input type="text"  class="am-radius" data-validation-message="请输入用户名" placeholder="用户名"  name="username" required/>
					<span class="am-input-group-label log-icon am-radius"><i class="am-icon-user am-icon-sm am-icon-fw"></i></span>
				</div>
				<br>
				<div class="am-input-group am-radius am-animation-slide-left">
					<input type="text"  class="am-radius" placeholder="昵称"  name="nickName"/>
					<span class="am-input-group-label log-icon am-radius"><i class="am-icon-user am-icon-sm am-icon-fw"></i></span>
				</div>
				<br>
				<div class="am-input-group am-animation-slide-left log-animation-delay">
					<input type="password" id="log-password" class="am-form-field am-radius log-input" placeholder="密码" minlength="6"  name="password" required>
					<span class="am-input-group-label log-icon am-radius"><i class="am-icon-lock am-icon-sm am-icon-fw"></i></span>
				</div>
				<br>
				<div class="am-input-group am-animation-slide-left log-animation-delay-a">
					<input type="password" data-equal-to="#log-password" class="am-form-field am-radius log-input" placeholder="确认密码" data-validation-message="请确认密码一致"  name="repassword" required>
					<span class="am-input-group-label log-icon am-radius"><i class="am-icon-lock am-icon-sm am-icon-fw"></i></span>
				</div>
				<br>
				<!--<div class="am-animation-slide-left log-animation-delay-a">-->
					<!--<select class=""am-form-field am-radius log-input name="gender" id="">-->
